{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "prerun": {
          "commands": [
            "npx prisma migrate deploy",
            "prisma generate"
          ]
        }
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ]
}
